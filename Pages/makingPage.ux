<Page ux:Class="makingPage">
	<Router ux:Dependency="router" />

    <JavaScript>
        var Check = require('CheckWifi');

        function check() {
            Check.CheckWifiJS();
        }

        module.exports = { check };
    </JavaScript>

    <StackPanel>
        <Button Text="Check" Clicked="{check}"/>
    </StackPanel>
</Page>